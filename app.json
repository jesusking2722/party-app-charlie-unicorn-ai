{
  "expo": {
    "name": "party-app-charlie-unicorn-ai",
    "slug": "party-app-charlie-unicorn-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "partyappcharlieunicornai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.vladgos.partyappcharlieunicornai",
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCZLK3UEmM2f96iDfQe-NsHB-gwqXTTxN8"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby events.",
        "LSApplicationQueriesSchemes": [
          "metamask",
          "trust",
          "safe",
          "rainbow",
          "uniswap",
          "metamask",
          "trust",
          "safe",
          "rainbow",
          "uniswap"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB7ey8dTcenIc_2dmADfMJ3jDIjA9Gsxsw"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.vladgos.partyappcharlieunicornai"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Charlie Party to use your location."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.partyappcharlieunicornai",
          "enableGooglePay": true
        }
      ],
      "./enableGooglePay.js",
      "./queries.js",
      "expo-web-browser",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.996856391179-39prl3561vq266fcf8ttua2kh9lhbl3q"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c53e1ef4-0ecc-477b-96e9-d439df580f0d"
      }
    },
    "owner": "vlad-gos"
  }
}
